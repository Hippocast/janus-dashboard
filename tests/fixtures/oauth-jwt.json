{
  "name": "jwt_auth",
  "oauth_endpoints": {
    "authorize": {
      "preserve_host": false,
      "listen_path": "/auth/authorize",
      "upstreams": {
        "balancing": "roundrobin",
        "targets": [
          {
            "target": "http://oauth_server/authorize",
            "weight": 0
          }
        ]
      },
      "insecure_skip_verify": false,
      "strip_path": false,
      "append_path": false,
      "enable_load_balancing": false,
      "methods": [
        "ALL"
      ],
      "hosts": []
    },
    "token": {
      "preserve_host": false,
      "listen_path": "/auth/token",
      "upstreams": {
        "balancing": "roundrobin",
        "targets": [
          {
            "target": "http://oauth_server/token",
            "weight": 0
          }
        ]
      },
      "insecure_skip_verify": false,
      "strip_path": false,
      "append_path": false,
      "enable_load_balancing": false,
      "methods": [
        "ALL"
      ],
      "hosts": []
    },
    "introspect": {
      "preserve_host": false,
      "listen_path": "",
      "upstreams": {
        "balancing": "",
        "targets": []
      },
      "insecure_skip_verify": false,
      "strip_path": false,
      "append_path": false,
      "enable_load_balancing": false,
      "methods": [],
      "hosts": []
    },
    "revoke": {
      "preserve_host": false,
      "listen_path": "/auth/revoke",
      "upstreams": {
        "balancing": "roundrobin",
        "targets": [
          {
            "target": "http://oauth_server/revoke",
            "weight": 0
          }
        ]
      },
      "insecure_skip_verify": false,
      "strip_path": false,
      "append_path": false,
      "enable_load_balancing": false,
      "methods": [
        "ALL"
      ],
      "hosts": []
    }
  },
  "oauth_client_endpoints": {
    "create": {
      "preserve_host": false,
      "listen_path": "",
      "upstreams": {
        "balancing": "",
        "targets": []
      },
      "insecure_skip_verify": false,
      "strip_path": false,
      "append_path": false,
      "enable_load_balancing": false,
      "methods": [],
      "hosts": []
    },
    "remove": {
      "preserve_host": false,
      "listen_path": "",
      "upstreams": {
        "balancing": "",
        "targets": []
      },
      "insecure_skip_verify": false,
      "strip_path": false,
      "append_path": false,
      "enable_load_balancing": false,
      "methods": [],
      "hosts": []
    }
  },
  "allowed_access_types": [],
  "allowed_authorize_types": [],
  "auth_login_redirect": "",
  "secrets": {
    "idone": "secretone",
    "idtwo": "secrettwo"
  },
  "cors_meta": {
    "domains": [
      "*"
    ],
    "methods": [
      "ALL"
    ],
    "request_headers": [
      "Origin",
      "Authorization",
      "Content-Type"
    ],
    "exposed_headers": [
      "X-Debug-Token",
      "X-Debug-Token-Link"
    ],
    "enabled": true
  },
  "rate_limit": {
    "limit": "240-S",
    "enabled": true
  },
  "token_strategy": {
    "name": "jwt",
    "settings": [
      {
        "alg": "HS256",
        "key": "mysecret"
      }
    ],
    "leeway": 0
  },
  "access_rules": []
}

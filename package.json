{
  "name": "janus-dashboard",
  "version": "0.0.1",
  "license": "MIT",
  "author": "",
  "dependencies": {
    "axios": "^0.16.2",
    "history": "^4.6.3",
    "node-sass-chokidar": "^0.0.3",
    "picklock": "^0.5.1",
    "ramda": "^0.24.1",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-history": "^0.18.2",
    "react-modaliz": "^0.1.2",
    "react-pagimagic": "^1.0.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "bem-cn": "^2.1.3",
    "npm-run-all": "^4.0.2",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.10",
    "react-scripts": "1.0.7",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "PORT=8082 npm-run-all -p watch-css start-js",
    "build": "npm run build-css && npm run lint:fix &&react-scripts build",
    "lint": "eslint --ext .jsx,.js ./src",
    "lint:fix": "eslint --ext .jsx,.js ./src --fix",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "pre-commit": [
    "lint"
  ]
}

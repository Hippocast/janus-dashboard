{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "extends": "eslint-config-airbnb",
    "rules": {
        "react/jsx-indent": 0,
        "react/react-in-jsx-scope": 0,
        "react/jsx-indent-props": 0,
        "no-use-before-define": 0,
        "import/prefer-default-export": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-no-bind": 0,

    },
    "plugins": [
        "babel",
        "react"
    ],
    "globals": {

    }
}
